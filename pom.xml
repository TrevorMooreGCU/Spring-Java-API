	<!-- Cloud Platform Profile configurations -->
	  <profiles>
	      <profile>
	         <id>dev</id>
	         <activation>
	            <activeByDefault>true</activeByDefault>
	         </activation>
	         <properties>
	             <build.profile.id>dev</build.profile.id>
	         </properties>
	         <build>
	           <resources>
	             <resource>
	               <directory>src/config/dev</directory>
	               <includes>
	                 <include>config.properties</include>
	               </includes>
	               <targetPath>config</targetPath>
	             </resource>
	             <resource>
	               <directory>src/config/dev</directory>
	               <includes>
	                 <include>web.xml</include>
	               </includes>
	               <targetPath>${project.basedir}/target/${warname}/WEB-INF</targetPath>
	             </resource>
	            <resource>
	               <directory>src/config/dev</directory>
	               <includes>
	                 <include>log4j.xml</include>
	               </includes>
	               <targetPath>../${warname}/WEB-INF/classes</targetPath>
	             </resource>
	           </resources>
	         </build>
	     </profile>
	      <profile>
	         <id>heroku</id>
	         <activation>
	            <activeByDefault>true</activeByDefault>
	         </activation>
	         <properties>
	             <build.profile.id>heroku</build.profile.id>
	         </properties>
	         <build>
	           <resources>
	             <resource>
	               <directory>src/config/heroku</directory>
	               <includes>
	                 <include>config.properties</include>
	               </includes>
	               <targetPath>config</targetPath>
	             </resource>
	             <resource>
	               <directory>src/config/heroku</directory>
	               <includes>
	                 <include>web.xml</include>
	               </includes>
	               <targetPath>${project.basedir}/target/${warname}/WEB-INF</targetPath>
	             </resource>
	            <resource>
	               <directory>src/config/heroku</directory>
	               <includes>
	                 <include>log4j.xml</include>
	               </includes>
	               <targetPath>../${warname}/WEB-INF/classes</targetPath>
	             </resource>
	           </resources>
	         </build>
	     </profile>
	  </profiles>
	
	
